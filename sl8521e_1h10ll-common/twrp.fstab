/recovery emmc /dev/block/platform/soc/by-name/recovery flags=display="Recovery";backup=1;flashimg=1
/lteruntimenv1 emmc /dev/block/platform/soc/by-name/l_runtimenv1 flags=display="Runtime NV 1";backup=1;flashimg=1
/lteruntimenv2 emmc /dev/block/platform/soc/by-name/l_runtimenv2 flags=display="Runtime NV 2";backup=1;flashimg=1
